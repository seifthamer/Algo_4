ALGORITHM insertion_sort
VAR

    set  : ARRAY_OF INTEGER[10];
    i,j , temp : INTEGER
    
BEGIN


    i = 0 ;
    while( i < set.lenght) do
        j = i+1 ;
        while( j > 0 ) do
            IF (arr[j] < arr[j-1]) THEN
                temp := arr [j]
                arr[j] := arr[j-1]
                arr [j-1] := temp
            END_IF
            j--
        END_WHILE
        i ++
    END_WHILE


END











